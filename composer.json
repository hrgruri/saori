{
    "name": "taniko/saori",
    "description": "static site generator",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Taniko\\Saori\\":"src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Test\\":"tests"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-mbstring": "*",
        "twig/twig": "^1.24",
        "cebe/markdown": "^1.1",
        "mibe/feedwriter": "^1.0",
        "symfony/console": "^3.1",
        "illuminate/validation": "^5.3",
        "illuminate/support": "^5.3",
        "symfony/yaml": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "fzaninotto/faker": "^1.6",
        "squizlabs/php_codesniffer": "^2.8",
        "mikey179/vfsStream": "^1.6"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit",
            "vendor/bin/phpcs -n src tests"
        ]
    }
}
